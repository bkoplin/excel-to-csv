{
  "name": "excel-to-csv",
  "type": "module",
  "version": "3.0.0",
  "packageManager": "pnpm@9.5.0",
  "description": "_description_",
  "license": "MIT",
  "funding": "https://github.com/sponsors/bkoplin",
  "homepage": "https://github.com/bkoplin/excel-to-csv#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkoplin/excel-to-csv.git"
  },
  "bugs": "https://github.com/bkoplin/excel-to-csv/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./dist/cli.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "esno ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "bundle": "unbuild && nexe --input ./dist/cli.mjs --output=\"excel-to-csv\" --build --python=\"$(which python3)\" --verbose --configure=--fully-static --make=-j9",
    "build": "unbuild",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && changelogithub",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "@commander-js/extra-typings": "^12.1.0",
    "@inquirer/prompts": "^5.3.8",
    "@types/lodash-es": "^4.17.12",
    "fast-glob": "^3.3.2",
    "inquirer-file-selector": "^0.4.0",
    "lodash-es": "^4.17.21",
    "magic-regexp": "^0.8.0",
    "papaparse": "^5.4.1",
    "picocolors": "^1.0.1",
    "type-fest": "^4.25.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yaml": "^2.5.0",
    "yocto-spinner": "^0.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@types/node": "^20.14.11",
    "@types/papaparse": "^5.3.14",
    "@vitest/ui": "^2.0.5",
    "bumpp": "^9.4.1",
    "changelogithub": "^0.13.10",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "nexe": "4.0.0-rc.6",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
